<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Gray shadow. -->
    <item android:left="3dp" android:top="5dp" >
        <shape>
            <solid android:color="@color/grad" />
            <corners android:topRightRadius="45sp" android:topLeftRadius="45dp" android:bottomLeftRadius="45sp" android:bottomRightRadius="45sp" />
        </shape>
    </item>
    <!-- White foreground. -->
    <item android:bottom="3dp" android:right="3dp"   android:start="5dp" android:top="7dp">
        <shape>
            <solid android:color="@color/bluee" />
            <corners android:topRightRadius="45sp" android:topLeftRadius="45dp" android:bottomLeftRadius="45sp" android:bottomRightRadius="45sp" />
        </shape>
    </item>
</layer-list>